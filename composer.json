{
    "name": "laverix-ec/laravel-acl",
    "description": "Role-based permissions for Laravel 5 built in Auth system.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "permissions",
        "roles",
        "auth",
        "acl",
        "security",
        "eloquent"
    ],
    "authors": [
        {
            "name": "Laverix Cia. Ltda.",
            "homepage": "https://github.com/Laverix-ec",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^5.0|^6.0",
        "doctrine/dbal": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Laverix\\Acl\\": "src/Laverix/Acl"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laverix\\Acl\\Tests\\": "tests/"
        }
    }
}
